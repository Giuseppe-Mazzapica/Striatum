{
    "name": "brain/striatum",
    "description": "Observer pattern package for WordPress hooks",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/Striatum/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/Striatum"
    },
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.4",
        "brain/brain": "dev-master",
        "brain/support": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "files": ["boot.php"],
        "psr-4": {
            "Brain\\Striatum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brain\\Striatum\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
