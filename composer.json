{
    "name": "brain/striatum",
    "description": "Observer pattern package and API for WordPress hooks.",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/Striatum/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/Striatum"
    },
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.4",
        "brain/brain": "dev-master",
        "brain/support": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master",
        "brain/hooks-mock": "dev-master"
    },
    "autoload": {
        "files": ["boot.php"],
        "psr-4": {
            "Brain\\Striatum\\": "src/"
        },
        "classmap": ["Hooks.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Brain\\Striatum\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
